        "cred012": "<Sonic Solveig>\uc5d0\uc11c \uace7 \ucd9c\uc2dc\ub420 \uc571\ub4e4", 
        "disc001": "\uc0b0\uc655\uc758 \ub3d9\uad74\uc5d0\uc11c", 
        "disc002": "\uc0b0\uc655 \uacf5\uc8fc\uc758 \ucda4", 
        "disc003": "\uc194\ubca0\uc774\uc9c0\uc758 \uc0ac\ub791 (\ucc3d\uc791\uace1)", 
        "disc004": "\uc624\uc81c\uc758 \uc8fd\uc74c", 
        "disc005": "\uc544\ub2c8\ud2b8\ub77c\uc758 \ucda4", 
        "disc006": "\uc194\ubca0\uc774\uc9c0\uc758 \ub178\ub798", 
        "disc007": "\ub09c\ud30c\uc120", 
        "disc008": "\uc194\ubca0\uc774\uadf8\uc758 \uc790\uc7a5\uac00", 
        "instru001": "\ubc14\uc774\uc62c\ub9b0\uc740 \ud604\uc545\uae30\uc758 \uc77c\uc885\uc774\uc608\uc694. \ud65c\uc740 \ub9d0\uc758 \uaf2c\ub9ac\uc778 \ub9d0\ucd1d\uc73c\ub85c \ub9cc\ub4dc\ub294\ub370, \uc904\uc744 \ucf1c\ub294 \uc5ed\ud560\uc744 \ud574\uc694. (\ubbf8, \ub77c, \ub808, \uc194) \ub610\ud55c \uae30\ud0c0\ucc98\ub7fc \uc190\uac00\ub77d\uc73c\ub85c \uc904\uc744 \ud295\uae38 \uc218 \uc788\uc5b4\uc694. \uadf8\uac78 \ud53c\uce58\uce74\ud1a0\ub77c\uace0 \ubd80\ub978\ub2f5\ub2c8\ub2e4. \ud53c\uce58\uce74\ud1a0\ub294 \uc774\ud0c8\ub9ac\uc544\uc5b4\ub85c \u2018\ubf51\ub294\u2019\ub2e4\ub294 \uc758\ubbf8\uc608\uc694.", 
        "instru002": "\ube44\uc62c\ub77c\ub294 \ud604\uc545\uae30\uc608\uc694. '\ub3c4', '\uc194', '\ub808', '\ub77c', \ub124 \uac1c\uc758 \uc904\ub85c \uad6c\uc131\ub418\uc5b4 \uc788\ub294\ub370, \ube44\uc62c\ub77c\ub294 \ubc14\uc774\uc62c\ub9b0\ubcf4\ub2e4 \uc6b8\ub9bc\ud1b5\uc774 \ud06c\uae30 \ub54c\ubb38\uc5d0 \ubc14\uc774\uc62c\ub9b0\ubcf4\ub2e4 \ub0ae\uc740 \uc18c\ub9ac\ub97c \ub0bc \uc218 \uc788\ub2f5\ub2c8\ub2e4. \ud65c\ub85c \ucf1c\ub294 \uac83 \ubfd0\ub9cc\uc544\ub2c8\ub77c \uc190\uc73c\ub85c \uc904\uc744 \ud295\uae38\uc218\ub3c4 \uc788\uc5b4\uc694.", 
        "instru003": "\uccbc\ub85c\ub294 \ube44\uc62c\ub77c\ubcf4\ub2e4 \ub354 \ub0ae\uc740 \uc74c\uc744 \ub0b4\uace0 \uc18c\ub9ac\ub3c4 \ub354 \ucee4\uc694. \uadf8\ub798\uc11c \uccbc\ub85c \uc18c\ub9ac\ub294 \uc0ac\ub78c \ubaa9\uc18c\ub9ac\uc640 \ube44\uc2b7\ud558\ub2e4\uace0 \ud55c\ub2f5\ub2c8\ub2e4. '\ub77c', '\ub808'. '\uc194'. '\ub3c4' \ub124 \uac1c\uc758 \uc904\uc774 \uc788\uace0, \ub2e4\ub978 \ud604\uc545\uae30\ucc98\ub7fc \ud65c \ubfd0\ub9cc\uc544\ub2c8\ub77c \uc190\uac00\ub77d\uc73c\ub85c \ud295\uaca8\uc11c\ub3c4 \uc5f0\uc8fc\ud560 \uc218\ub3c4 \uc788\uc5b4\uc694.", 
        "instru004": "\ucf58\ud2b8\ub77c\ubca0\uc774\uc2a4\ub294 \ub9e4\uc6b0 \ub0ae\uc740 \uc74c\uc744 \ub0b4\ub294 \ud604\uc545\uae30\uc608\uc694. \ucf58\ud2b8\ub77c\ubca0\uc774\uc2a4\ub294 \ub369\uce58\uac00 \uc544\uc8fc \ud06c\uae30 \ub54c\ubb38\uc5d0 \uc5f0\uc8fc\ub97c \ud558\ub824\uba74 \uc77c\uc5b4\uc11c\uac70\ub098 \ub192\uc740 \uc758\uc790\uc5d0 \uc549\uc544\uc57c \ud574\uc694. \uc7ac\uc988\uc5f0\uc8fc\uac00\ub294 \uc885\uc885 \uc190\uac00\ub77d\uc73c\ub85c \ud604\uc744 \ud295\uaca8 \uc5f0\uc8fc\ud574\uc694. '\uc194', '\ub808', '\ub77c', '\ubbf8' \ub124 \uac1c\uc758 \uc904\ub85c \uad6c\uc131\ub418\uc5b4 \uc788\ub2f5\ub2c8\ub2e4.", 
        "instru005": "\uc0c9\uc18c\ud3f0\uc740 \ubaa9\uad00 \uc545\uae30\uc608\uc694. \ub9c8\uc6b0\uc2a4\ud53c\uc2a4\ub97c \uc785\uc5d0 \ubb3c\uace0 \ubc14\ub78c\uc744 \ubd88\uc5b4\ub123\uc73c\uba74 \ub9ac\ub4dc\uac00 \ub5a8\uba74\uc11c \uc18c\ub9ac\uac00 \ub09c\ub2f5\ub2c8\ub2e4. \uc0c9\uc18c\ud3f0\uc5d0\ub294 4\uac00\uc9c0 \uc885\ub958\uac00 \uc788\uc5b4\uc694. \uadf8 \uc911, \ubc14\ub9ac\ud1a4 \uc0c9\uc18c\ud3f0\uc740 \uac00\uc7a5 \ub0ae\uc740 \uc18c\ub9ac\ub97c \ub0b4\uace0, Eb\uc74c\uae4c\uc9c0 \ub0bc \uc218 \uc788\uc5b4\uc694.", 
        "instru006": "\ubc14\uc21c\uc740 \ubaa9\uad00\uc545\uae30\uc778\ub370\uc694, \ud2b9\uc774\ud558\uac8c \ub9ac\ub4dc\ub97c \ub450 \uc7a5\uc774\ub098 \uc368\uc694. \ub0ae\uc740 \uc18c\ub9ac\ub97c \ub0b4\uae30 \ub54c\ubb38\uc5d0 \uc8fc\ub85c \ud654\uc74c\uc758 \uc544\ub798\ub97c \ubc1b\ud600\uc8fc\ub294 \uc5ed\ud560\uc744 \ud574\uc694. \ud558\uc9c0\ub9cc \ub3c5\uc8fc\ub3c4 \ud558\uace0 \ube44\ub974\ud22c\uc624\uc18c(\uace0\ub3c4\uc758 \uae30\uad50\ub97c \ubcf4\uc5ec\uc8fc\ub294) \uc791\ud488\ub3c4 \uc5f0\uc8fc\ud560 \uc218\ub3c4 \uc788\uc5b4\uc694.", 
        "instru007": "\uc789\uae00\ub9ac\uc2dc \ud638\ub978\uc740 \uc774\ub984\uc740 \ud638\ub978\uc774\ub77c \ud558\uc9c0\ub9cc \uc0ac\uc2e4 \uc624\ubcf4\uc5d0\uc758 \ud55c \uc885\ub958\uc608\uc694. \ubcf8\ub798 \uc624\ubcf4\uc5d0\ubcf4\ub2e4 \ub0ae\uc740 '\ud30c' \uc74c\uc744 \ub0bc \uc218 \uc788\uc744 \ub54c\ubd80\ud130 \uc774\ub984\uc774 \uc774\ub807\uac8c \ubd99\uc5c8\ub2f5\ub2c8\ub2e4. \uc789\uae00\ub9ac\uc2dc \ud638\ub978\uc774\ub780 \uc774\ub984\uc740 \ub3c5\uc77c\uc5b4 \u2018\ucc9c\uc0ac\uc758 \ubfd4\u2019\uc774 \uc798\ubabb \uc804\ud574\uc838 \uc9c0\uae08\uae4c\uc9c0 \uc654\ub2e4\uace0\ub4e4 \ub9d0\ud55c\ub2f5\ub2c8\ub2e4.", 
        "instru008": "\uc624\ubcf4\uc5d0\ub3c4 \ud2b9\uc774\ud558\uac8c \ubc14\uc21c\uacfc \uac19\uc774 \ub450 \uc7a5\uc758 \ub9ac\ub4dc\ub97c \uac00\uc9c4 \ubaa9\uad00 \uc545\uae30\uc608\uc694. \uc624\ubcf4\uc5d0\ub294 \ub9e4\uc6b0 \uc624\ub798\uc804\ubd80\ud130 \uc874\uc7ac\ud588\ub358 \uc545\uae30\ub78d\ub2c8\ub2e4. \uace0\ub300 \uc774\uc9d1\ud2b8\uc5d0\uc11c \u201c\ubbf8\uc988\ub9c8\ub974\u201d\ub77c\uace0 \ubd88\ub838\uace0, \uace0\ub300 \uadf8\ub9ac\uc2a4\uc5d0\uc11c\ub294 \u201c\uc544\uc6b8\ub85c \uc544-\u201d\ub77c\uace0 \ubd88\ub800\uc5b4\uc694. \ud48d\ubd80\ud55c \uc74c\uc0c9\uc744 \uc790\ub791\ud558\ub294 \uc545\uae30\ub78d\ub2c8\ub2e4. \uadf8\ub9ac\uace0 \uc624\ucf00\uc2a4\ud2b8\ub77c\ub294 \ud56d\uc0c1 \uc624\ubcf4\uc5d0\uc758 '\ub77c' \uc74c\uc5d0 \ub9de\ucdb0 \uc870\uc728\ud55c\ub2f5\ub2c8\ub2e4.", 
        "instru009": "\ud074\ub77c\ub9ac\ub137\uc740 \ubaa9\uad00 \uc545\uae30\uc608\uc694. \ub2e4\ub978 \ubaa9\uad00\uc545\uae30\uc640 \ub9c8\ucc2c\uac00\uc9c0\ub85c \ub9c8\uc6b0\uc2a4\ud53c\uc2a4\uc5d0 \ubc14\ub78c\uc744 \ubd88\uc5b4\ub123\uc5b4 \ub9ac\ub4dc\ub97c \uc9c4\ub3d9\uc2dc\ucf1c \uc18c\ub9ac\ub97c \ub0b4\uc694. \ud074\ub77c\ub9ac\ub137\uc740 \uc0ec\ub958\ubaa8\ub77c\ub294 \uace0\ub300 \uc545\uae30\uc5d0\uc11c \ube44\ub86f\ub418\uc5c8\ub2f5\ub2c8\ub2e4. ", 
        "instru010": "\ud50c\ub86f\uc740 \uae08\uc18d\uc73c\ub85c \ub9cc\ub4e4\uc5b4\uc84c\uc74c\uc5d0\ub3c4 \ubd88\uad6c\ud558\uace0, \ubaa9\uad00 \uc545\uae30\ub85c \ubd84\ub958\ud574\uc694. \uace0\ub300 \uc2dc\ub300\uc5d0\ub294 \ub098\ubb34\ub85c \ub9cc\ub4e4\uc5c8\uae30 \ub54c\ubb38\uc774\uc8e0. \uae08\uc18d\uc784\uc5d0\ub3c4 \ubd88\uad6c, \ubaa9\uad00\uc545\uae30 \ud2b9\uc720\uc758 \uc601\ub871\ud55c \uc18c\ub9ac\ub97c \ub0b4\uc694. \uc74c\uc5ed\ub300\uac00 \ub192\uae30 \ub54c\ubb38\uc5d0 \uc624\ucf00\uc2a4\ud2b8\ub77c \ud569\uc8fc \ub54c\uc5d0\ub3c4 \ub610\ub837\ud558\uac8c \ub4e4\uc744 \uc218 \uc788\uc5b4\uc694.", 
        "instru011": "\ud53c\ucf5c\ub85c\ub294 \uc774\ud0c8\ub9ac\uc544 \uc5b4\ub85c \u2018\uc791\uc740\u2019\uc774\ub780 \uc758\ubbf8\uc608\uc694. \ud06c\uae30\ub294 \ud50c\ub8fb\uc758 \uc808\ubc18 \uc815\ub3c4\uc778\ub370 \uc74c\uc5ed\uc774 \ub9e4\uc6b0 \ub192\ub2f5\ub2c8\ub2e4.", 
        "instru012": "\ud300\ud30c\ub2c8\ub294 \uac00\uc8fd\uc744 \ub113\uac8c \ud3bc\uccd0 \ub9cc\ub4e0 \ud0c0\uc545\uae30\ub85c, \ub9ec\ub9bf\uc774\ub77c \ubd80\ub974\ub294 \ub9c9\ub300\ub85c \uac00\uc8fd\uc744 \ub54c\ub824 \uc18c\ub9ac\ub97c \ub0b8\ub2f5\ub2c8\ub2e4. \uc0dd\uae40\uc0c8\uc640\ub294 \ub2e4\ub974\uac8c \uc74c\ub192\uc774\ub97c \uc870\uc808\ud560 \uc218 \uc788\ub2f5\ub2c8\ub2e4. \ud398\ub2ec\uc744 \uc870\uc808\ud558\uac70\ub098, \uac00\uc7a5\uc790\ub9ac\ub97c \uc190\uc73c\ub85c \ub9cc\uc838\uc11c \ub9d0\uc774\uc8e0.", 
        "instru013": "\ud0ec\ubc84\ub9b0\uc740 \uc9c0\uc911\ud574\uc5d0\uc11c \uc720\ub798\ud55c \ud0c0\uc545\uae30 \uc911 \ud558\ub098\uc608\uc694. \uc190\uc73c\ub85c \uce58\ub294 \uac00\uc8fd \ubd80\ubd84\uacfc \uc9e4\ub791\uc774\ub294 \uae08\uc18d \ubd80\ubd84\uc73c\ub85c \uc774\ub8e8\uc5b4\uc838 \uc788\uc5b4\uc694. \uc548\ub2ec\ub8e8\uc2dc\uc544 \uc9c0\ubc29\uc758 \ud0c0\ub974\ub77c\ub294 \uc545\uae30\uc640 \ub2ee\uc558\ub2f5\ub2c8\ub2e4.", 
        "instru014": "\uce90\uc2a4\ud130\ub124\uce20\ub294 \uc2a4\ud398\uc778\uc758 \ud50c\ub77c\ub9f9\uace0 \ucda4\uc5d0 \ub9e4\uc6b0 \ub110\ub9ac \uc4f0\uc774\ub294 \ud0c0\uc545\uae30\ub78d\ub2c8\ub2e4. \ubb34\uc6a9\uc218\uc758 \ucda4 \ub3d9\uc791\uc774 \ubc14\ub014 \ub54c\ub9c8\ub2e4 \uce90\uc2a4\ud130\ub124\uce20\ub97c \uc5f0\uc8fc\ud558\uc8e0. \uce90\uc2a4\ud130\ub124\uce20\ub294 \ud0c0\uc545\uae30 \uc911\uc5d0\uc11c\ub3c4 \uccb4\uba85\uc545\uae30\ub85c \ubd84\ub958\ub418\ub294\ub370\uc694, \uccb4\uba85\uc545\uae30\ub780 \uc545\uae30 \ubcf8\uccb4\ub97c \ube44\ube44\uac70\ub098 \ub54c\ub824 \uc545\uae30 \uc790\uccb4\uac00 \uc18c\ub9ac\ub97c \ub9cc\ub4e4\uc5b4 \ub0b4\ub294 \uc545\uae30\ub97c \uc77c\uceec\uc5b4 \ub9d0\ud55c\ub2f5\ub2c8\ub2e4.", 
        "instru015": "\ud2b8\ub77c\uc774\uc575\uae00\uc740 \ud0c0\uc545\uae30 \uc911\uc5d0\uc11c\ub3c4 \uc545\uae30 \uc790\uc2e0\uc758 \ub9c8\ucc30\uc774\ub098 \ud0c0\uaca9\uc73c\ub85c \uc18c\ub9ac\uac00 \ub098\ub294 \uccb4\uba85\uc545\uae30\uc608\uc694. \ub9c9\ub300\ub85c \uc0b4\uc9dd \uce58\uae30\ub9cc \ud574\ub3c4 \uc18c\ub9ac\uac00 \ub9e4\uc6b0 \uac15\ud558\uace0 \uc624\ub798\uac00\uae30 \ub54c\ubb38\uc5d0 \uc18c\ub9ac\ub97c \uadf8\ub9cc \ub0b4\ub824\uba74 \uc190\uc73c\ub85c \uc7a1\uc544\uc918\uc57c \ud574\uc694.", 
        "instru016": "\uc2ec\ubc8c\uc988\ub294 \uae08\uc18d\uc73c\ub85c \ub9cc\ub4e4\uc5b4\uc9c4 \ud0c0\uc545\uae30\uc608\uc694. \uc624\ucf00\uc2a4\ud2b8\ub77c\uc5d0\uc11c \ud798\uc788\uace0 \ud3ed\ubc1c\uc801\uc778 \uc18c\ub9ac\ub97c \ub0bc \ub54c \uc0ac\uc6a9\ud55c\ub2f5\ub2c8\ub2e4.", 
        "instru017": "\ubc34\uc870\ub294 4\uac1c\uc758 \uc904\ub85c \ub9cc\ub4e4\uc5b4\uc9c4 \uc791\uc740 \uae30\ud0c0\uc608\uc694. \uc774 \uc5b4\ud50c\ub9ac\ucf00\uc774\uc158\uc5d0\uc11c \ub4e4\uc744 \uc218 \uc788\ub294 \ubc34\uc870 \uc18c\ub9ac\ub294 \ube0c\ub93c\uc140\uc758 \ud55c \ubcbc\ub8e9\uc2dc\uc7a5\uc5d0\uc11c \uc5f0\uc8fc\ud55c \uac70\uc608\uc694. '\ub808', '\uc194', '\uc2dc', '\ub808', 4\uac1c\uc758 \uc904\uc774 \uc788\uc5b4\uc694.", 
        "instru018": "\uc904\uc744 \ud295\uaca8 \uc5f0\uc8fc\ud558\ub294 \uae30\ud0c0\ub294 '\ubbf8', '\ub77c', '\ub808', '\uc194', '\uc2dc', '\ubbf8', 6\uac1c\uc758 \uc904\ub85c \uc774\ub8e8\uc5b4\uc838\uc788\uc5b4\uc694. \ud48d\ubd80\ud55c \ud654\uc74c\uc744 \ub0bc \uc218 \uc788\uae30 \ub54c\ubb38\uc5d0 \uc8fc\ub85c \ud654\uc74c\ubd80\ub97c \ub9e1\uc9c0\ub9cc, \ub9ac\ub4ec\uc774\ub098 \uba5c\ub85c\ub514\ub97c \ub9e1\uc544 \uc5f0\uc8fc\ud558\uae30\ub3c4 \ud574\uc694.", 
        "instru019": "\ud638\ub978\uc740 \ud2b8\ub7fc\ud3ab, \ud2b8\ub86c\ubcf8, \ud29c\ubc14\uacfc \uac19\uc740 \uae08\uad00 \uc545\uae30\ub78d\ub2c8\ub2e4. \uba3c \uacf3\uc5d0\uc11c\ub3c4 \ub4e4\uc744 \uc218 \uc788\ub294 \uc0ac\ub0e5 \ubfd4\uc5d0\uc11c \uc2dc\uc791\ub418\uc5c8\uc5b4\uc694. \ub465\uae00\ub465\uae00\ud55c \uc74c\uc0c9 \ub355\ubd84\uc5d0 \uc624\ucf00\uc2a4\ud2b8\ub77c\uc5d0\uc11c\ub294 \ub2e4\ub978 \uc545\uae30\uc758 \uc74c\uc744 \uc774\uc5b4\uc8fc\ub294 \uc5ed\ud560\uc744 \ud574\uc694.", 
        "ctxt0001": "\ud398\ub974 \uadc4\ud2b8\ub294 \ub204\uad74\uae4c\uc694?", 
        "ctxt0002": "\ud5e8\ub9ad \uc785\uc13c\uc740 \uc5b4\ub5a4 \uc0ac\ub78c\uc77c\uae4c\uc694?", 
        "ctxt0003": "\uc5d0\ub4dc\ubc14\ub974 \uadf8\ub9ac\uadf8\ub294 \uc5b4\ub5a4 \uc0ac\ub78c\uc774\uc5c8\uc744\uae4c\uc694?", 
        "ctxt0004": "\uc774 \uc5b4\ud50c\ub9ac\ucf00\uc774\uc158\uc5d0\uc11c \uc0ac\uc6a9\ud55c \uc545\uae30\ub4e4", 
        "ctxt0005": "\uc2a4\uce78\ub514\ub098\ube44\uc544\uc758 \ubbfc\uac04\uc804\uc2b9", 
        "ctxt1000": "\ud398\ub974 \uadc4\ud2b8", 
        "ctxt1001": "\ud398\ub974 \uadc4\ud2b8\ub294 \ub204\uad6c\uc77c\uae4c\uc694?", 
        "ctxt1002": "\ud398\ub974 \uadc4\ud2b8\ub294 \uac70\uc9d3\ub9d0\uc7c1\uc774\uc608\uc694...", 
        "ctxt1003": "\ud398\ub974 \uadc4\ud2b8\ub294 \ud5e8\ub9ad \uc785\uc13c\uc774 \ub9cc\ub4e0 \uc2dc\uadf9\uc758 \uc8fc\uc778\uacf5\uc774\uc608\uc694. 1876\ub144 \uc774 \uc2dc\ub294 \uc5f0\uadf9\uc73c\ub85c \ub2e4\uc2dc \uc4f0\uc5ec\uc838 \ub178\ub974\uc6e8\uc774 \uc624\uc2ac\ub85c\uc5d0\uc11c \ucc98\uc74c \ub9c9\uc744 \uc62c\ub838\uc5b4\uc694. \uac70\uae30\uc5d0 \uc5d0\ub4dc\ubc14\ub974 \uadf8\ub9ac\uadf8\uac00 \uace1\uc744 \ubd99\uc5ec \uc720\uba85\ud55c \ubba4\uc9c0\uceec \ud398\ub974 \uadc4\ud2b8\uac00 \ud0c4\uc0dd\ud558\uac8c \ub418\uc5c8\uc5b4\uc694.", 
        "ctxt1003_1": "20\uc0b4 \ud398\ub974 \uadc4\ud2b8\ub294 \ub9e4\uc0ac\uc5d0 \ubd88\ub9cc\uc774 \uac00\ub4dd \ucc28 \uc788\ub294 \uc18c\ub144\uc774\uc5c8\uc5b4\uc694. \uadf8\ub294 \ub9e4\uc0ac\uc5d0 \ubaa8\ub4e0 \uac83\uc744 \ubf50\ub0b4\uba70, \uadf8\ub7ec\uba74\uc11c\ub3c4 \uac81\uc7c1\uc774\uace0, \uac70\uc9d3\ub9d0\uc7c1\uc774\uba70, \uc545\ub3d9\uc774\uc5c8\uc5b4\uc694. \ub299\uc740 \uc5b4\uba38\ub2c8 \uc544\uc138\uc640 \uc0b4\uace0 \uc788\ub358 \uadf8\ub294 \uc57c\ub9dd\uc5d0 \uac00\ub4dd\ucc28\uc788\uc5c8\uc5b4\uc694.", 
        "ctxt1004": "\ud398\ub974 \uadc4\ud2b8\uc758 \uc0ac\ub791...", 
        "ctxt1005 ": "\uc5b4\ub290 \ub0a0, \uc0ac\ub791\uc2a4\ub7f0 \uc194\ubca0\uc774\uc9c0\ub294 \ud398\ub974\uc640 \uc0ac\ub791\uc5d0 \ube60\uc838 \uadf8\uc640 \uacb0\ud63c\ud558\uace0 \uc2f6\uc5b4\ud588\uc5b4\uc694. \ud558\uc9c0\ub9cc \ud398\ub974\ub294 \uc194\ubca0\uc774\uc9c0\ub97c \ubc84\ub9ac\uace0 \ubd80\uc720\ud55c \ub18d\ubd80\uc640 \uc57d\ud63c\ud55c \uc544\ub984\ub2e4\uc6b4 \uc18c\ub140 \uc789\uadf8\ub9ac\ub4dc\ub97c \ub0a9\uce58\ud558\ub294 \uc77c\uc744 \uc800\uc9c8\ub800\uc5b4\uc694. \ud558\uc9c0\ub9cc \uc774\uac83\uc740 \uc55e\uc73c\ub85c \uc774\uc5b4\uc9c8 \ub098\uc05c \ud589\ub3d9\uacfc \uae30\uc774\ud55c \ubaa8\ud5d8\uc758 \uc2dc\uc791\uc5d0 \ubd88\uacfc\ud574\uc694.", 
        "ctxt1006": "\ud398\ub974 \uadc4\ud2b8\uc758 \uc720\ud639...", 
        "ctxt1007": "\ud398\ub974\ub294 \uc232\uc5d0\uc11c \ub179\uc0c9 \uc637\uc744 \uc785\uc740 \uc5ec\uc778\uc744 \ub9cc\ub098\uc790\ub9c8\uc790 \uc789\uadf8\ub9ac\ub4dc\ub97c \ubc84\ub9ac\uace0 \ub5a0\ub098\uac00 \ubc84\ub838\uc5b4\uc694. \ub179\uc0c9 \uc637\uc758 \uc5ec\uc778\uc740 \ud398\ub974\uc640 \uc0ac\ub791\uc5d0 \ube60\uc84c\uace0, \uadf8\ub97c \uc790\uc2e0\uc758 \uc544\ubc84\uc9c0\uc778 \ud2b8\ub864\uc758 \uc655\uc5d0\uac8c \uc18c\uac1c\ud558\uae30\uc5d0 \uc774\ub974\ub800\uc5b4\uc694.", 
        "ctxt1008": "\ud2b8\ub864\uc5d0\uac8c\uc11c \ub3c4\ub9dd\uce58\ub294 \ud398\ub974 \uadc4\ud2b8...", 
        "ctxt1009": "\uc5b4\ub9ac\uc11d\uc740 \ud398\ub974\ub294 \ubd80\uc790\uac00 \ub418\uace0 \uc2f6\uc740 \uc695\uc2ec\uc5d0 \ub179\uc0c9 \uc637\uc758 \uc18c\ub140\uc640\uc758 \uacb0\ud63c \uc9c0\ucc38\uae08\uc73c\ub85c \uc655\uc758 \uc790\ub9ac\ub97c \ub2ec\ub77c\uace0 \ud588\uc5b4\uc694. \ud558\uc9c0\ub9cc \uc794\uc778\ud558\uace0 \ube44\uc5f4\ud55c \ud2b8\ub864\uc758 \uc655\uc740 \uc81c\uc548\uc744 \uc218\ub77d\ud558\ub294 \ub300\uc2e0 \ud398\ub974\uc758 \uc5c9\ub369\uc774\uc5d0 \ub3fc\uc9c0\uaf2c\ub9ac\ub97c \ub2ec\uace0, \uadf8\uc758 \ub208 \ud55c \ucabd\uc744 \uba40\uac8c \ud558\uc5ec \ud2b8\ub864\uc758 \ubaa8\uc2b5\uc73c\ub85c \ubc14\uafd4\ubc84\ub838\uc5b4\uc694. \uc790\uc2e0\uc758 \ubaa8\uc2b5\uc5d0 \ub180\ub780 \ud398\ub974 \uadc4\ud2b8\ub294 \ub3c4\ub9dd\uc744 \ucce4\uace0, \ud2b8\ub864\uc740 \uadf8\ub97c \ucad3\uae30 \uc2dc\uc791\ud588\uc5b4\uc694.", 
        "ctxt1010": "\ub2e4\uc2dc \ub5a0\ub098\ub294 \ud398\ub974 \uadc4\ud2b8", 
        "ctxt1011": "\uadf8 \ud6c4 \ud398\ub974\ub294 \uc194\ubca0\uc774\uc9c0\ub97c \ub2e4\uc2dc \ub9cc\ub0ac\uc5b4\uc694. \ud558\uc9c0\ub9cc \ud398\ub974 \uadc4\ud2b8\ub294 \ubab8\uc774 \ub9ce\uc774 \ud3b8\ucc2e\uc544\uc9c4 \uc5b4\uba38\ub2c8 \uc544\uc138\ub97c \ub3cc\ubcf4\uae30 \uc704\ud574 \uc194\ubca0\uc774\uc9c0 \uacc1\uc744 \uc7a0\uc2dc \ub5a0\ub098\uc57c \ud588\uc5b4\uc694. \ub2e4\uc2dc \ub9cc\ub09c \uc5b4\uba38\ub2c8\uc5d0\uac8c \ud398\ub974\ub294 \uadf8\ub4e4\uc774 \uc0c1\uc0c1\uc758 \uc370\ub9e4\ub97c \ud0c0\uace0 \uc18c\ub9ac\uc544-\ubaa8\ub9ac\uc544 \uc131\uc73c\ub85c \ub0a0\ub77c\uac00\ub294 \uc774\uc57c\uae30\ub97c \ub4e4\ub824\uc8fc\uc5c8\uc5b4\uc694.", 
        "ctxt1012": "\ud398\ub974 \uadc4\ud2b8\uc758 \ubaa8\ud5d8", 
        "ctxt1013": "\ud398\ub974\ub294 \ubaa8\ub4e0 \uac83\uc744 \ub0b4\ub824 \ub193\uace0 \uba40\ub9ac \ub5a8\uc5b4\uc9c4 \uc544\ud504\ub9ac\uce74 \ub300\ub959\uc73c\ub85c \ud5a5\ud574\uc694. \uadf8 \uacf3\uc5d0\uc11c \ud398\ub974\ub294 \ub178\uc608 \uc0c1\uc778\uc774 \ub418\uc5b4 \ubd80\ub97c \uc5bb\uace0 \uc608\uc5b8\uc790\ub85c\uc11c \uc0ac\ub791\uc744 \ubc1b\uac8c \ub3fc\uc694. \ud558\uc9c0\ub9cc \uc5ec\uc804\ud788 \ud398\ub974\ub294 \uadf8\uc758 \uc0b6\uc5d0 \ub9cc\uc871\ud560 \uc904\uc744 \ubab0\ub77c\uc694. \ud56d\uc0c1 \ubb34\uc5c7\uc744 \ub354 \uc6d0\ud560\uc218\ub85d \ub9c8\uc9c0\ub9c9\uc5d4 \ubaa8\ub4e0 \uac83\uc744 \uc783\uac8c \ub3fc\uc694. \ub05d\ub0b4\ub294 \ubaa8\ub85c\ucf54\uc5d0\uc11c \uc544\ub984\ub2e4\uc6b4 \uc544\ub2c8\ud2b8\ub77c\uc640 \ubc29\ud0d5\ud55c \uc0dd\ud65c\uc5d0 \ube60\uc838 \uc804 \uc7ac\uc0b0\uc744 \ud0d5\uc9c4\ud558\uac8c \ub418\uc5c8\ub2f5\ub2c8\ub2e4.", 
        "ctxt1014": "\uac00\ub77c\uc549\uc740 \ud398\ub974 \uadc4\ud2b8\uc758 \ubc30..", 
        "ctxt1015": "\ubaa8\ub4e0 \uac83\uc744 \ub2e4 \uc783\uc5b4 \ub354 \uc774\uc0c1 \uc783\uc744 \uac83\uc774 \uc5c6\uc5b4\uc9c4 \ud398\ub974\ub294 \uc9d1\uc73c\ub85c \ub3cc\uc544\uac00\ub294 \ubc30\uc5d0 \ubab8\uc744 \uc2e4\uc5c8\uc5b4\uc694. \ud5a5\uc218\uc5d0 \uc7a0\uaca8 \ud398\ub974\ub294 \uadf8\uc758 \uc0ac\ub791\uc2a4\ub7f0 \uc194\ubca0\uc774\uc9c0\ub97c \uc0dd\uac01\ud574\uc694. \uadf8\ub7f0\ub370 \ud398\ub974\ub294 \ubc14\ub2e4 \ud55c \uac00\uc6b4\ub370\uc5d0\uc11c \ub054\ucc0d\ud55c \ud3ed\ud48d\uc744 \ub9de\uc544 \uce68\ubab0\ud558\uac8c \ub418\uc5c8\uc5b4\uc694. \uba3c\uc800 \ud0c8\ucd9c\ud55c \ud398\ub974\ub294 \ub3c4\uc6c0\uc744 \uae30\ub2e4\ub9ac\ub294 \uc120\uc6d0\ub4e4\uc758 \uc190\uc744 \ubfcc\ub9ac\uce58\uace0 \ud55c \uba85\ub3c4 \ub3c4\uc640\uc8fc\uc9c0 \uc54a\uc544\uc694.", 
        "ctxt1016": "\ud398\ub974 \uadc4\ud2b8, \uc5ec\uae30 \uc7a0\ub4e4\ub2e4", 
        "ctxt1017": "\ud398\ub974 \uadc4\ud2b8\ub294 \uc815\ub9d0 \ub204\uad6c\uc77c\uae4c\uc694? \uadf8\ub294 \uac81\uc7c1\uc774\uc77c\uae4c\uc694? \ud0d0\ud5d8\uac00\uc77c\uae4c\uc694? \uadf8\ub294 \uc790\uad6d\ubbfc\uc5d0\uac8c\uc11c\ub294 \ube44\ub09c\uc744 \ubc1b\uc558\uc9c0\ub9cc, \uc5ec\ud589\ud558\ub294 \ub3d9\uc548\ub9cc\ud07c\uc740 \ub2e4\ub978 \uc0ac\ub78c\uc5d0\uac8c \uc874\uacbd\uc758 \ub300\uc0c1\uc774\uc5c8\uc5b4\uc694. \uc194\ubca0\uc774\uc9c0\ub294 \uccab\uc0ac\ub791 \ud398\ub974 \uadc4\ud2b8\ub97c \uc218\uc2ed\ub144 \ub3d9\uc548 \uae30\ub2e4\ub824\uc654\ub2e4\uac00 \uacb0\uad6d\uc740 \ub458\uc774 \ub9cc\ub098\uac8c \ub418\uc5c8\uc5b4\uc694. \uc194\ubca0\uc774\uc9c0\ub294 \ud398\ub974\uac00 \uc8fd\uae30 \uc9c1\uc804 \uc790\uc7a5\uac00\ub97c \ubd88\ub7ec\uc90d\ub2c8\ub2e4. \ud398\ub974\ub294 \uc8fd\uae30 \uc9c1\uc804, \uc0b6\uc758 \uc758\ubbf8\ub97c \uae68\ub2eb\uc2b5\ub2c8\ub2e4. \u201c\ub098\ub294 \uc774\ubcf4\ub2e4 \ub354 \ud589\ubcf5\ud560 \uc218 \uc5c6\ub2e4. \ub098\ub294 \uc9c0\uae08\uae4c\uc9c0 \ub0b4 \uc6b4\uba85\uc758 \uc218\uc218\uaed8\ub07c\ub97c \ud480\uc5b4\uc654\ub2e4. \ub0b4\uac00 \uc790\uc2e0\uc744 \ub290\ub080 \uc774\ud6c4 \ubaa8\ub4e0 \uac83\uc740 \uc6a9\uc11c\ubc1b\uc558\ub2e4. \uc874\uc7ac\ub85c\uc11c\uc758 \ub098, \ud398\ub974 \uadc4\ud2b8\ub77c\ub294 \ub098, \uc0b6\uc758 \ud669\uc81c\ub77c \uc790\uc2e0\ud588\ub358 \ub098.\u201d", 
        "ctxt2000": "\ud5e8\ub9ad \uc785\uc13c(1828-1906)", 
        "ctxt2001": "\ubc18 \uc138\uae30\uc5d0 \uac78\uccd0, \ud5e8\ub9ad \uc785\uc13c\uc740 \uadf8\uc758 \uc0b6\uacfc \uc5d0\ub108\uc9c0\ub97c \uc5f0\uadf9\uc5d0 \ud5cc\uc2e0\ud588\uc5b4\uc694. \uadf8\ub294 \ub2f9\ub300\uc758 \ud6cc\ub96d\ud55c \uc791\uac00\uc600\ub2f5\ub2c8\ub2e4. 1864\ub144 \uadf8\ub294 \ubcf4\uc870\uae08\uc744 \ubc1b\uac8c \ub418\uc5b4 \uc870\uad6d \ub178\ub974\uc6e8\uc774\ub97c \ub5a0\ub098 \ub85c\ub9c8\uc640 \ud30c\ub9ac\uc5d0 \uba38\ubb34\ub974\uac8c \ub418\uc5c8\ub294\ub370, \uadf8\uac83\uc740 \uadf8\uc758 \ub098\uc774 27\uc138 \ub54c \uc600\uc5b4\uc694. \uadf8\ub294 \uc720\ub7fd \uc804\uc5ed\uc5d0 \uc774\ub984\uc744 \ub5a8\ucce4\uc5b4\uc694. \uadf8\uac00 \uc138\uc0c1\uc744 \ub5a0\ub09c \ub4a4\uc5d0\ub3c4, \uadf8\uc758 \uc5f0\uadf9\ub4e4\uc740 \ub3c5\uc77c \uc804\uc5ed\uc5d0\uc11c \uacf5\uc5f0\ub418\uc5c8\uc5b4\uc694. \uadf8\ub294 \ub178\ub974\uc6e8\uc774\uc801\uc778 \uac83\ub4e4\uc774 \ub2e4\ub978 \ub098\ub77c\uc5d0\uc11c\ub3c4 \ud1b5\ud55c\ub2e4\ub294 \uc0ac\uc2e4\uc744 \uc54c\uc558\uc5b4\uc694. \uadf8\ub9ac\uace0 \uadf8\uc758 \ub098\uc774 50\uc5d0\ub294 \uadf8\ub9ac\uadf8\uc5d0\uac8c (\ub2f9\uc2dc 30\ub300) \uc0c8 \uc5f0\uadf9, \ud398\ub974 \uadc4\ud2b8\uc758 \uc74c\uc545\uc744 \uc368 \ub2ec\ub77c\uace0 \ubd80\ud0c1\ud588\uc5b4\uc694.", 
        "ctxt2002": "\uc5f0\uadf9\uc740 \uacf5\uc804\uc758 \ud788\ud2b8\ub97c \ucce4\uc9c0\ub9cc, \ub458\uc740 \uacb0\ucf54 \ub9cc\uc871\ud558\uc9c0 \uc54a\uc558\uc5b4\uc694. \uc785\uc13c\uc740 \uadf8\ub9ac\uadf8\uc758 \uc74c\uc545\uc774 \ub108\ubb34 \ube44\uc5b4\uc788\ub2e4\uace0 \uc0dd\uac01\ud588\uace0, \uadf8\ub9ac\uadf8\ub294 \u201c\ub098\ub294 \uc774 \uc5f0\uadf9\uc744 \uadf8 \uc548\uc758 \uc815\uc2e0\uacfc \ub3c5\uc124 \ub54c\ubb38\uc5d0 \uc874\uacbd\ud558\uc9c0\ub9cc \uacb0\ucf54 \uc88b\uc544\ud560 \uc218\ub294 \uc5c6\uc744 \uac83\uc774\ub2e4. \uadf8\ub7ec\ub098 \uc785\uc13c\uc774 \uc4f4 \uc791\ud488 \uc911 \ucd5c\uace0\ub77c \uc0dd\uac01\ud55c\ub2e4.\u201d\ub77c\ub294 \ub9d0\ub85c \ube44\ub09c \uc544\ub2cc \ube44\ub09c\uc744 \ud588\uc5b4\uc694.", 
        "ctxt3000": "\uc5d0\ub4dc\ubc14\ub974 \uadf8\ub9ac\uadf8 (1843-1907)", 
        "ctxt3001": "\uadf8\ub9ac\uadf8\ub294 \ub178\ub974\uc6e8\uc774\uc758 \uc791\uace1\uac00\uc774\uc790 \ub0ad\ub9cc\uc8fc\uc758 \uc2dc\ub300\uc758 \uc9c0\ud718\uc790\uc600\uc5b4\uc694. \uadf8\uc758 \uc5b4\uba38\ub2c8\ub294 \ud53c\uc544\ub2c8\uc2a4\ud2b8\uc600\uace0, \uadf8\uc758 \uc544\ubc84\uc9c0\ub294 \uc678\uad50\uad00\uc774\uc5c8\uc73c\uba70, \uadf8\uc758 \ud560\uc544\ubc84\uc9c0\ub294 \ubca0\ub974\uac90 \uc624\ucf00\uc2a4\ud2b8\ub77c\uc758 \ub2e8\uc6d0\uc774\uc5c8\uc5b4\uc694. \uadf8\ub9ac\uadf8\ub294 5 \ub0a8\ub9e4 4\uc9f8\ub85c \ud0dc\uc5b4\ub0ac\uc5b4\uc694. \uadf8\ub9ac\uadf8\uac00 10\ub300\uc600\uc744 \ub54c, \uadf8\ub294 \ubc14\uadf8\ub108\uc640 \uc288\ub9cc\uc758 \uc74c\uc545\uc5d0 \uc2ec\ucde8\ud574\uc788\uc5c8\uace0, \ud604\uc545 4\uc911\uc8fc \uace1\ub3c4 \uc4f0\uae30 \uc2dc\uc791\ud588\uc5b4\uc694. \uc790\uc2e0\uc758 \uc5f0\uc8fc\ud68c \ub54c\uc5d0\ub294 \ubca0\ud1a0\ubca4\uacfc \uc288\ub9cc\uc758 \uace1\ub3c4 \uc5f0\uc8fc\ud560 \uc815\ub3c4\ub85c \ub6f0\uc5b4\ub09c \ud53c\uc544\ub2c8\uc2a4\ud2b8\uae30\ub3c4 \ud588\uace0\uc694.", 
        "ctxt3002": "\uadf8\uc758 \uc544\ub0b4, \ub2c8\ub098 \ud558\uac8c\ub8f9\uc740 \uc131\uc545\uac00\uc600\uc5b4\uc694. \uadf8\ub140\ub294 \ub6f0\uc5b4\ub09c \ubaa9\uc18c\ub9ac\ub97c \uac00\uc9c0\uc9c4 \uc54a\uc558\uc9c0\ub9cc, \ud6cc\ub96d\ud55c \uac10\uc218\uc131\uc73c\ub85c \ub0a8\ud3b8\uc758 \uace1\uc744 \ub178\ub798\ud588\uc5b4\uc694. 1874\ub144 \uc785\uc13c\uc740 \uadf8\ub9ac\uadf8\uc5d0\uac8c \uc0c8 \uc5f0\uadf9 \ud398\ub974 \uadc4\ud2b8\uc758 \uc74c\uc545\uc744 \uc368\ub2ec\ub77c\uace0 \ubd80\ud0c1\ud588\uc5b4\uc694. \ub450 \uc0ac\ub78c\uc758 \ud611\uc5c5\uc740 \ub374\ub9c8\ud06c\uc758 \uc2dd\ubbfc\uc9c0\uc2dc\uc808 \ub178\ub974\uc6e8\uc774 \uc608\uc220\uc758 \uc815\uccb4\uc131\uacfc \ucc3d\uc870\uc131\uc744 \ub300\ud45c \uc0ac\ub840\ub85c \uaf3d\ud78c\ub2f5\ub2c8\ub2e4. \uadf8\ub9ac\uadf8\ub294 \ud398\ub974 \uadc4\ud2b8\ub97c \uc791\uace1\ud558\ub294\ub370 \uaf2c\ubc15 2\ub144\uc774 \uac78\ub838\uc5b4\uc694. \ud604\uc7ac \uadf8\uc758 \uc74c\uc545\uc740 \uc804\uc138\uacc4\uc801\uc73c\ub85c \uc720\uba85\ud569\ub2c8\ub2e4. \uc885\uc885 \uad11\uace0\uc5d0 \uc0ac\uc6a9\ub418\uc8e0. \ud504\ub9ac\uce20 \ub791\uc758 \uc601\ud654 <M>\uc5d0\uc11c \uc8fc\uc778\uacf5\uc740 \ud718\ud30c\ub78c\uc73c\ub85c \u201c\uc0b0\uc655\uc758 \ub3d9\uad74\uc5d0\uc11c\u201d\ub97c \ub530\ub77c\ubd80\ub974\uae30\ub3c4 \ud558\uace0\uc694. \uc7ac\uc988 \uc5f0\uc8fc\uc790 \ub4c0\ud06c \uc5d8\ub9c1\ud134\uc740 \ud398\ub974 \uadc4\ud2b8 \uc870\uace1 1, 2\ubc88\uc744 \ud3b8\uace1\ud558\uc5ec \uc5f0\uc8fc\ud558\uae30\ub3c4 \ud588\uc5b4\uc694.", 
        "ctxt4000": "\ud5e8\ub9ad \uc785\uc13c (1828-1906)", 
        "ctxt4001": "\uc2a4\uce78\ub514\ub098\ube44\uc544\uc758 \ubbfc\uac04\uc804\uc2b9", 
        "ctxt4002": "\uc2a4\uce78\ub514\ub098\ube44\uc544\uc758 \ubbfc\uac04\uc804\uc2b9\uc740 \uad11\ud65c\ud55c \uc232\uc5d0\uc11c\ubd80\ud130 \ube44\ub86f\ub418\uc5c8\ub2f5\ub2c8\ub2e4. \uc624\ub85c\ub77c\ub3c4 \uc124\ud654\uc5d0 \uc601\uac10\uc744 \uc8fc\ub294 \uc18c\uc7ac \uc911 \ud558\ub098\uc8e0. \uc232\uc5d0\ub294 \uc628\uac16 \ucc29\ud558\uace0 \ub098\uc05c \ub3d9\ubb3c\ub4e4\uc774 \uc655\uc655 \uc0b4\uace0 \uc788\uc5b4\uc694. \uc5ec\uae30\uc5d0 \ud398\ub974 \uadc4\ud2b8\uc758 \ubc14\ud0d5\uc774 \ub41c \ub178\ub974\uc6e8\uc774 \uc2e0\ud654\uc758 \ud2b9\uc774\ud55c \uc774\uc57c\uae30\uac00 \uc788\uc5b4\uc694.", 
        "ctxt4003": "\uc232", 
        "ctxt4004": "\uc232\uc740 \uafc8\uacfc \uc545\ubabd\uc744 \ub3d9\uc2dc\uc5d0 \uafc0 \uc218 \uc788\ub294 \uacf3\uc774\uc608\uc694. \ub098\ubb34\ub294 \ud558\ub298\uacfc \uc9c0\uad6c \uc0ac\uc774\ub97c \uc5f0\uacb0\uc2dc\ucf1c\uc8fc\ub294 \uace0\ub9ac\ub85c \uc5ec\uacbc\ub2f5\ub2c8\ub2e4. \ud398\ub974\uac00 \uc789\uadf8\ub9ac\ub4dc\ub97c \ub0a9\uce58\ud55c \ud6c4 \uc232\uc5d0 \ubab8\uc744 \uc228\uae30\uc8e0. \ub179\uc0c9 \uc5ec\uc778\uc744 \ub9cc\ub09c \uacf3\ub3c4 \ub610\ud55c \uc232\uc774\uc5c8\uace0\uc694.", 
        "ctxt4005": "\u201c\uc6b0\ub9ac\ub294 \ub2f9\uc2e0\uc774 \ud758\ub824\uc57c \ud588\ub358 \ubaa8\ub4e0 \ub208\ubb3c\uc785\ub2c8\ub2e4\u201d\ub77c\ub294 \uc774\uc2ac\ubc29\uc6b8\uc758 \uc18d\uc0ad\uc784\uc744 \ub4e4\uc5c8\ub358 \uacf3, \ub610 \u201c\uc6b0\ub9ac\ub294 \ub2f9\uc2e0\uc774 \ud588\uc5b4\uc57c \ud588\ub358 \uc0dd\uac01\ub4e4\uc785\ub2c8\ub2e4\u201d\ub77c\ub294 \ud138\uc2e4\ubb49\uce58\uc758 \ub098\uc9c0\ub9c9\ud55c \uc678\uce68\uc744 \ub4e4\uc740 \uacf3 \ub610\ud55c \uc232 \uc18d\uc774\uc608\uc694. \ub9c8\uc9c0\ub9c9\uc73c\ub85c \ub2e4\ub9ac\uc5d0 \ub204\uc6cc \ub2ec\ucf64\ud55c \uc194\ubca0\uc774\uc9c0\uc758 \ubaa9\uc18c\ub9ac\ub97c \ub4e4\uc73c\uba70 \ud398\ub974\uc758 \ub9c8\uc9c0\ub9c9\uc744 \uc9c0\ucf30\ub358 \uacf3 \uc5ed\uc2dc \uc232\uc18d\uc785\ub2c8\ub2e4. \uc55e\uc5d0\uc11c \uc5b8\uae09\ud55c \uc232\uc740 \ub3c4\ube0c\ub808(\ub178\ub974\uc6e8\uc774 \uc911\uc2ec\ubd80\uc5d0 \uc788\ub294 \uc0b0\ub9e5) \uc0b0\ub9e5 \uadfc\ucc98\uc5d0 \uc788\ub2f5\ub2c8\ub2e4.", 
        "ctxt4006": "\ud2b8\ub864", 
        "ctxt4007": "\ud2b8\ub864\uc740 \uc790\uc5f0\uc758 \ubb34\uc2dc\ubb34\uc2dc\ud55c \ud798\uc744 \uac16\uace0 \uc788\uc9c0\ub9cc, \uba4d\uccad\ud55c \uac70\uc778\uc73c\ub85c \uc885\uc885 \uc54c\uace0 \uc788\uc8e0. \uc0b0\uc744 \ub290\ub9bf\ub290\ub9bf \ubc30\ud68c\ud558\uba70 \uc0b0\ub2f5\ub2c8\ub2e4. \ud558\uc9c0\ub9cc \ub178\ub974\uc6e8\uc774\uac00 \uc810\ucc28\uc801\uc73c\ub85c \uae30\ub3c5\uad50\ub97c \ubc1b\uc544\ub4e4\uc784\uc5d0 \ub530\ub77c \ud2b8\ub864\uc740 \uc810\uc810 \uc545\ud55c \uc874\uc7ac\ub85c \ubb18\uc0ac\ub418\uace0, \uc6d0\ub798 \uc758\ubbf8\ub97c \uc783\uac8c \ub418\uc5c8\uc5b4\uc694. \uadf8\ub798\ub3c4 \ud2b8\ub864\ub4e4\uc740 \uc694\uc998\ub3c4 \ub3c4\ube0c\ub808\uc758 \ub208 \ub36e\ud78c \uc0b0\uc744 \ubca0\ud68c\ud560\uac70\uc608\uc694.", 
        "ctxt4007_1": "\uc774\uc57c\uae30\uc758 \ub05d\uc5d0\uc11c \ud398\ub974\ub294 \uac70\ub300\ud55c \ud2b8\ub864\uc744 \ub9cc\ub098\uac8c \ub418\uc5b4\uc694. \uc774 \ud06c\uace0 \uad6c\ubd80\uc815\ud55c \ud2b8\ub864\uc740 \uc0ac\uc2e4\uc740 \uc2a4\uce78\ub514\ub098\ube44\uc544\uc758 \uad6c\ub4dc\ube0c\ub780\ub4dc\uc2a4\ub2ec\uacfc \ud154\ub808\ub9c8\ub974\ud06c(\ub178\ub974\uc6e8\uc774 \ub0a8\ubd80\uc5d0 \uc788\ub294 \uc0b0\uc774 \ub9ce\uc740 \uc8fc)\uc758 \uc124\ud654\uc5d0\uc11c \ub098\uc624\ub294 \ud2b8\ub864\uc774\uc608\uc694. \uc5d0\ud2b8\ub124\ub2ec\uc758 \uac70\ub300\ud55c \ud2b8\ub864\uc73c\ub85c\ub3c4 \ubd88\ub824\uc694. \uac70\uc778\uc774\ub098 \ub0a0\uc52c\ud55c \ubc40\uc73c\ub85c \ubb18\uc0ac\ud558\ub4ef\uc774, \uc5ec\ud589\uc790\ub4e4\uc758 \uc7a5\uc560\ubb3c \uc815\ub3c4\uc758 \uc874\uc7ac\uc608\uc694.", 
        "ctxt4008": "\uc0b0\uc655", 
        "ctxt4009": "\uc0b0\uc655\uc740 \ud398\ub974 \uadc4\ud2b8\uc758 \ubaa8\ud5d8\uc5d0 \ub098\uc624\ub294 \uc911\uc694\ud55c \ub4f1\uc7a5 \uc778\ubb3c\uc774\uc608\uc694. \uc0b0\uc655\uc740 \ub3c4\ube0c\ub808 \uc0b0\ub9e5\uc5d0 \uc788\ub294 \ud2b8\ub864\uc758 \uc655\uad6d\uc744 \uc9c0\ubc30\ud574\uc694. \ud398\ub974\ub294 \uadf8\uc758 \ub538\uacfc\uc758 \uacb0\ud63c\uc744 \ub178\ub9ac\uace0 \uc804\uccb4 \uc655\uad6d\uc744 \uc9c0\ucc38\uae08\uc73c\ub85c \uc694\uad6c\ud588\uc5c8\uc5b4\uc694. \uadf8\ub9ac\uace0 \uc655\uc740 \u201c\uc801\ub2f9\ud788 \ub2f9\uc2e0 \ub73b\ub300\ub85c\u201d\ub77c \ub9d0\ud588\uc5b4\uc694. \uc0b0\uc655\uc740 \ud398\ub974 \uadc4\ud2b8\uc758 \uc5c9\ub369\uc774\uc5d0 \ub3fc\uc9c0 \uaf2c\ub9ac\uac00 \uc790\ub77c\ub098\uac8c \ud588\uc5b4\uc694. \uadf8\ub294 \ub610\ud55c \ud398\ub974 \uadc4\ud2b8\uc758 \ub208\uc744 \uba40\uac8c\ud558\uc5ec \ud398\ub974 \uadc4\ud2b8\uac00 \uc9c4\uc9dc \ud2b8\ub864\ucc98\ub7fc \ubabb\uc0dd\uaca8 \ubcf4\uc774\ub3c4\ub85d \ub9cc\ub4e4\uace0 \uc2f6\uc5c8\uc5b4\uc694. \uc18c\ub625 \uba39\uae30\ub97c \uac15\uc694\ub2f9\ud558\uae30\ub3c4 \ud588\uc5c8\uc5b4\uc694. \uacb0\uad6d \ud398\ub974\ub294 \uc9d0\uc744 \ub4e4\uace0 \uc0b0\uc73c\ub85c\ubd80\ud130 \ub3c4\ub9dd\ucce4\uc5b4\uc694. \uc0b0\uc774 \ub2e4 \ubb34\ub108\uc838\ub0b4\ub9ac\uae30 \uc804, \ud2b8\ub864\uc740 \ud398\ub974\ub97c \ucad3\uc544 \ub098\uc654\uc5b4\uc694.", 
        "legal001": "\uac74\uac15 \uacbd\uace0", 
        "legal002": "\uc774 \uc751\uc6a9 \ud504\ub85c\uadf8\ub7a8\uc758 \uacf5\uacf5 \ubc0f \uac1c\uc778 \uc0ac\uc6a9\uc5d0 \ub300\ud574", 
        "legal003": "3\uc138 \uc774\uc804\uc5d0\ub294 \uc544\uc774\uac00 \ud63c\uc790 \uc2a4\ud06c\ub9b0\uc744 \uc0ac\uc6a9\ud558\uc9c0 \uc54a\ub294 \uac83\uc744 \uad8c\uace0\ud569\ub2c8\ub2e4.\n\ud5e4\ub4dc\uc14b \uc5c6\uc774 \uc774 \uc5b4\ud50c\ub9ac\ucf00\uc774\uc158\uc744 \uc0ac\uc6a9\ud558\ub294 \uac83\uc744 \ucd94\ucc9c\ud569\ub2c8\ub2e4. \ud5e4\ub4dc\uc14b\uc73c\ub85c \ud070 \uc18c\ub9ac\ub97c \uc624\ub798 \ub4e4\uc744 \uc2dc \uc5b4\ub9b0\uc774\uc5d0\uac8c\ub294 \ub09c\uccad\uc744 \uc720\ubc1c\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.", 
        "legal004": "\uc751\uc6a9 \ud504\ub85c\uadf8\ub7a8\uc758 \uac1c\uc778\uc801 \uc0ac\uc6a9 \uc774\uc678\uc758 \ub2e4\ub978 \uc0ac\uc6a9\uc740 \uc5c4\uaca9\ud788 \uae08\uc9c0\ub429\ub2c8\ub2e4. \uac00\uc871\uacfc \uce5c\uad6c \uc678\uc5d0\ub294 \uc74c\uc545\uc758 \uc0ac\uc6a9\uacfc \ubc29\uc1a1\uc744 \uae08\ud569\ub2c8\ub2e4. \uc608\ub97c \ub4e4\uc5b4 \uce74\ud398, \ub808\uc2a4\ud1a0\ub791, \uac00\uac8c, \ub300\uae30\uc2e4, \uae30\ucc28\uc5ed, \uacf5\ud56d \ub77c\uc6b4\uc9c0, \uad50\ud1b5 \uc218\ub2e8, \ubbf8\ub514\uc5b4 \ub3c4\uc11c\uad00, \ub3c4\uc11c\uad00 \ub610\ub294 \uc5b4\ub5a4 \uc885\ub958\uc758 \uc6b4\ub3d9 \uc911\uc774\ub098, \uc5d4\ud130\ud14c\uc778\uba3c\ud2b8, \uad50\uc721\uc801\uc774\uac70\ub098 \ub808\ud06c\ub9ac\uc5d0\uc774\uc158 \ud589\uc0ac, \uc800\ub141\uc2dd\uc0ac, \ubc14\ube44\ud050, \ud53c\ud06c\ub2c9, \uacf5\uacf5\uc5d0\uac8c \uac1c\ubc29\ub41c \uc5f0\ud68c\uc5d0\uc11c \uc774 \uc5b4\ud50c\ub9ac\ucf00\uc774\uc158\uc758 \uc0ac\uc6a9\uc744 \uae08\ud569\ub2c8\ub2e4. \uc774 \uc751\uc6a9 \ud504\ub85c\uadf8\ub7a8\uc744 \ub610\ub294 \uc74c\uc545\uc744 \uce5c\uad6c\ub098 \uac00\uc871 \uc774\uc678\uc758 \uc0ac\ub78c\ub4e4\uacfc \uc0ac\uc6a9\ud558\uace0 \uc2f6\ub2e4\uba74 Sonic Solveig\uce21\uc758 \ud5c8\uac00\ub97c \uc5bb\uc5b4\uc57c \ud569\ub2c8\ub2e4. contact@sonicsolveig.com\uc73c\ub85c \uba54\uc77c\uc744 \ubcf4\ub0b4 \uc694\uccad\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4. \uac01\uac01\uc758 \uae30\uc900\uc5d0 \ub530\ub77c \uad6c\uccb4\uc801\uc778 \uc694\uae08\uc774 \ucc45\uc815\ub429\ub2c8\ub2e4. (\uc774\ubca4\ud2b8\uc758 \uc885\ub958, \uc7a5\uc18c\uc758 \ud06c\uae30, \uc0ac\uc6a9 \uae30\ud55c \ub4f1)"
    }
}